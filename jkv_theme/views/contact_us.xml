<odoo>
    <template id="jkv_theme_contactus" inherit_id="website.contactus" priority="999">
        <xpath expr="//div[@id='wrap']" position="replace">
            <t t-set="additional_title">Contact Us</t>
            <div id="wrap">
                <div class="oe_structure"/>
                <section class="top-content">
                    <div class="main-banner"></div>
                    <div class="container child-banner">
                        <div class="row">
                            <div class="col-md-6">
                                <div class="flex-col flex-auto">
                                    <div class="flex-item">
                                        <h4 class="breadcrumbs">
                                            <a href="">HOME Â»</a>
                                            <span>Contact Us</span>
                                        </h4>
                                        <h2>Contact Us</h2>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </section>
                <section class="main-content">
                    <div class="container">
                        <div class="row">
                            <div class="col-md-7">
                                <div class="box-form mg-b-30">
                                    <div class="tit-page-spacing">
                                        <div class="box-tit">
                                            <h3 class="tit">CONTACT JK VIDEO</h3>
                                            <span class="required">*Required</span>
                                        </div>
                                        <hr class="dark"/>
                                    </div>
                                    <form class="form-horizontal s_website_form" action="/website_form/" method="post"
                                          data-model_name="crm.lead"
                                          data-success_page="/page/website_crm.contactus_thanks"
                                          enctype="multipart/form-data" id="jkv_form_contact">

                                        <div class="row form-field">
                                            <label class="col-sm-3 control-label">WHO YOU ARE?
                                            </label>
                                            <div class="col-sm-9">
                                                <div class="tit-page-spacing">
                                                    <div class="form-check">
                                                        <input class="form-control o_website_form_input" type="checkbox"
                                                               id="is_rider" name="is_rider" />
                                                        <label class="form-check-label" for="is_rider">
                                                            Rider
                                                        </label>
                                                    </div>

                                                    <div class="form-check">
                                                        <input class="form-control o_website_form_input" type="checkbox"
                                                             id="is_trainer" name="is_trainer" />
                                                        <label class="form-check-label" for="is_trainer">
                                                            Trainer
                                                        </label>
                                                    </div>

                                                    <div class="form-check">
                                                        <input class="form-control o_website_form_input" type="checkbox"
                                                              id="is_owner" name="is_owner"/>
                                                        <label class="form-check-label" for="is_owner">
                                                            Owner
                                                        </label>
                                                    </div>

                                                    <div class="form-check">
                                                        <input class="form-check-input form-control o_website_form_input" type="checkbox"
                                                              id="is_event_producer_manager" name="is_event_producer_manager"/>
                                                        <label class="form-check-label" for="is_event_producer_manager">
                                                            Event producer/manager
                                                        </label>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>

                                        <div class="row form-field o_website_form_required_custom">
                                            <label class="col-sm-3 control-label" for="first_name">Name<span class="text-danger">*</span>
                                            </label>
                                            <div class="col-sm-9">
                                                <div class="row">
                                                    <div class="col-sm-6">
                                                        <div class="form-group">
                                                            <input type="text" class="form-control o_website_form_input"
                                                                   placeholder="First" name="first_name"
                                                                   required="required"/>
                                                        </div>
                                                    </div>
                                                    <div class="col-sm-6">
                                                        <div class="form-group">
                                                            <input type="text" class="form-control o_website_form_input"
                                                                   placeholder="Last"
                                                                   name="last_name"
                                                                   required="required"/>
                                                        </div>
                                                    </div>
                                                </div>

                                            </div>
                                        </div>


                                        <div class="row form-group form-field o_website_form_required_custom">
                                            <label class="col-sm-3 control-label" for="phone">Phone
                                            </label>
                                            <div class="col-sm-9">
                                                <input type="tel" class="form-control o_website_form_input"
                                                       name="phone" id="phone"/>
                                            </div>
                                        </div>
                                        <div class="row form-group form-field o_website_form_required_custom">
                                            <label class="col-sm-3 control-label" for="email_from">Email<span class="text-danger">*</span>
                                            </label>
                                            <div class="col-sm-9">
                                                <input type="email" class="form-control o_website_form_input"
                                                       name="email_from" required="required"/>
                                            </div>
                                        </div>
                                        <div class="row form-group form-field">
                                            <label class="col-sm-3 control-label" for="name">Event
                                            </label>
                                            <div class="col-sm-9">
                                                <input type="text" class="form-control o_website_form_input"
                                                       name="name"/>
                                            </div>
                                        </div>
                                        <div class="row form-group form-field">
                                            <label class="col-sm-3 control-label" for="description">DETAILS ABOUT THE
                                                EVENT
                                            </label>
                                            <div class="col-sm-9">
                                                <textarea class="form-control o_website_form_input"
                                                          name="description"></textarea>
                                            </div>
                                        </div>
                                        <div class="row form-group">
                                            <div class="col-sm-12">
                                                <hr/>
                                            </div>
                                        </div>
                                        <div class="row form-group">
                                            <div class="col-sm-9 col-sm-offset-3">
                                                <a href="" class="btn btn-block btn-form o_website_form_send">
                                                    <i class="pull-right">
                                                        <svg version="1.1" x="0px" y="0px" width="6px"
                                                             viewBox="0 0 12.13 22.208"
                                                             enable-background="new 0 0 12.13 22.208"
                                                             xml:space="preserve"><polyline fill="none" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" stroke-miterlimit="10" points="  1.095,1.133 11.095,11.133 1.095,21.135 "></polyline></svg>
                                                    </i>
                                                    Send
                                                </a>
                                                <span id="o_website_form_result"></span>
                                            </div>
                                        </div>
                                    </form>
                                    <div class="se-pre-con" style="display:none"></div>
                                </div>
                            </div>

                            <div class="col-md-5">
                                <div class="top-right-corner">
                                    <div class="row">
                                        <div class="col-lg-11 col-lg-offset-1 col-md-12 col-md-offset-0">
                                            <div class="box-form has-pattern">
                                                <div class="box-tit">
                                                    <h3 class="tit">Contact DETAILS</h3>
                                                </div>
                                                <div class="detail-info">
                                                    <h4 class="tit">JK VIDEOGRAPHY</h4>
                                                    <div>Austin TX 78729</div>
                                                    <div>United States</div>
                                                    <div class="mg-b-30"></div>

                                                    <h4 class="tit">PHONE (EMERGENCIES ONLY)</h4>
                                                    <div>(512) 809-7797</div>
                                                    <div class="mg-b-30"></div>

                                                    <h4 class="tit">EMAIL</h4>
                                                    <div>jkvideotx@gmail.com</div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </section>
                 <t t-if="call_to_action">
                    <section class="pre-foot">
                    <div class="container">
                        <div class="row text-center">
                            <div class="col-md-12">
                                <div class="tit-spacing">
                                    <h4 class="pre-tit">SCHEDULE A CONSULTATION</h4>
                                    <h2 class="main-tit">BRING THE POWER OF VIDEO TO YOUR SHOW</h2>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-4 col-md-offset-4 col-sm-6 col-sm-offset-3">
                                <a href="/page/contactus" class="btn btn-main btn-white btn-block">
                                    <i class="pull-right">
                                        <svg version="1.1" x="0px" y="0px" width="6px" viewBox="0 0 12.13 22.208"
                                             enable-background="new 0 0 12.13 22.208" xml:space="preserve">
            <polyline fill="none" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" stroke-miterlimit="10"
                      points="  1.095,1.133 11.095,11.133 1.095,21.135 "></polyline>
            </svg>
                                    </i>
                                    Contact Us
                                </a>
                            </div>
                        </div>
                    </div>
                </section>
                 </t>
                <div class="oe_structure"/>
            </div>
        </xpath>
    </template>

</odoo>